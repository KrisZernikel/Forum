itle: {\n        flexGrow: 1,\n    },\n}));\n\nexport default ({ user, signOut, src }) => {\n    const classes = useStyles();\n    useEffect(() => {\n        const _saveUser = async () => {\n            console.log(user._json.first_name)\n            await axios({\n                method: 'put',\n                url: '/api/dynamo/save-user',\n                data: {\n                    email: user.emails[0].value,\n                    firstName: user._json.first_name,\n                    lastName: user._json.last_name\n                }\n            })\n        }\n        _saveUser()\n    }, [])\n    return (\n        <div className={classes.root}>\n            <AppBar position=\"static\">\n                <Toolbar>\n                    <Typography variant=\"h6\" className={classes.title}>\n                        Forum\n                    </Typography>\n                    <Button color=\"inherit\" onClick={signOut}>Sign out</Button>\n                </Toolbar>\n            </AppBar>\n            <main style={{ padding: \"1rem\" }}>\n                <PostBox src={src} />\n            </main>\n        </div>\n    )\n}"},"lineCount":null}},"error":null,"hash":"e2f24d0b349660a6fff10c8cd3e08841","cacheData":{"env":{}}}